{
  "name": "quiprendquoi",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "babel-preset-minify": "^0.5.1",
    "babelify": "^10.0.0",
    "concurrently": "^5.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "start": "concurrently --kill-others -n \"sass,server,watchify,sw\" -c \"magenta,cyan,red,green\" \"nodemon app.js\" \"npm:sass\" \"npm:watchify\" \"npm:sw\"",
    "sass": "node-sass styles/style.scss public/style.css -w --output-style compressed",
    "watchify": "watchify scripts/app.js -o public/script.js -t [ babelify --presets [ @babel/preset-env babel-preset-minify ] ]",
    "sw": "watchify sw.js -o public/sw.js -t [ babelify --presets [ babel-preset-minify ] ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rorrotapia/quiprendquoi.git"
  },
